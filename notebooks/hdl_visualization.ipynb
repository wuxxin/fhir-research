   "source": [
    "# HDL Cholesterol Visualization (Jupyter Notebook)\\n",
    "\\n",
    "This notebook demonstrates:\\n",
    "1. Generating FHIR data for HDL cholesterol observations using `fhir_utils`.\\n",
    "2. Flattening the FHIR data into a pandas DataFrame.\\n",
    "3. Visualizing the HDL values over time using Bokeh."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\\n",
    "from bokeh.plotting import figure, show\\n",
    "from bokeh.models import DatetimeTickFormatter\\n",
    "from bokeh.io import output_notebook\\n",
    "from datetime import datetime, timedelta, date # Added date for consistency\\n",
    "import json\\n",
    "\\n",
    "# Assuming Jupyter is run from the project root, or src is in PYTHONPATH\\n",
    "import sys\\n",
    "import os\\n",
    "# Add project root to path to allow import from src, if not already discoverable\\n",
    "if os.path.abspath(os.path.join(os.getcwd(), os.pardir)) not in sys.path:\\n",
    "    sys.path.append(os.path.abspath(os.path.join(os.getcwd(), os.pardir)))\\n",
    "\\n",
    "from src.medical_data_science.fhir_utils import (\\n",
    "    create_patient_lab_bundle,\\n",
    "    flatten_fhir_bundle,\\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 2: Initialize Bokeh for Notebook\\n",
    "output_notebook()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 3: Data Generation (Updated for refactored fhir_utils)\\n",
    "\\n",
    "# Define Patient Data\\n",
    "patient_details = {\\n",
    "    'id': \"patient-jupyter-01\",\\n",
    "    'family_name': \"Jenson\",\\n",
    "    'given_name': \"Jupyter\",\\n",
    "    'birth_date': \"1975-08-20\",\\n",
    "    'gender': \"male\"\\n",
    "}\\n",
    "\\n",
    "# Define HDL Observations Data (for create_observation via create_patient_lab_bundle)\\n",
    "lab_observations_details = [\\n",
    "    {\\n",
    "        'observation_id': \"hdl-jupyter-01\",\\n",
    "        'effective_date_time': \"2019-11-01T09:00:00Z\",\\n",
    "        'code_system': \"http://loinc.org\", 'code': \"2085-9\", 'code_display': \"Cholesterol in HDL\",\\n",
    "        'value_quantity_value': 45.0, 'value_quantity_unit': \"mg/dL\", 'value_quantity_unit_code': \"mg/dL\"\\n",
    "    },\\n",
    "    {\\n",
    "        'observation_id': \"hdl-jupyter-02\",\\n",
    "        'effective_date_time': \"2020-12-05T09:30:00Z\",\\n",
    "        'code_system': \"http://loinc.org\", 'code': \"2085-9\", 'code_display': \"Cholesterol in HDL\",\\n",
    "        'value_quantity_value': 48.0, 'value_quantity_unit': \"mg/dL\", 'value_quantity_unit_code': \"mg/dL\"\\n",
    "    },\\n",
    "    {\\n",
    "        'observation_id': \"hdl-jupyter-03\",\\n",
    "        'effective_date_time': \"2021-10-15T08:45:00Z\",\\n",
    "        'code_system': \"http://loinc.org\", 'code': \"2085-9\", 'code_display': \"Cholesterol in HDL\",\\n",
    "        'value_quantity_value': 42.0, 'value_quantity_unit': \"mg/dL\", 'value_quantity_unit_code': \"mg/dL\"\\n",
    "    },\\n",
    "    {\\n",
    "        'observation_id': \"hdl-jupyter-04\",\\n",
    "        'effective_date_time': \"2022-11-20T10:00:00Z\",\\n",
    "        'code_system': \"http://loinc.org\", 'code': \"2085-9\", 'code_display': \"Cholesterol in HDL\",\\n",
    "        'value_quantity_value': 51.0, 'value_quantity_unit': \"mg/dL\", 'value_quantity_unit_code': \"mg/dL\"\\n",
    "    }\\n",
    "]\\n",
    "\\n",
    "# Generate FHIR Bundle\\n",
    "print(\"Generating FHIR Bundle using refactored functions...\")\\n",
    "fhir_bundle = create_patient_lab_bundle(\\n",
    "    patient_details=patient_details,\\n",
    "    lab_observations_details=lab_observations_details\\n",
    ")\\n",
    "print(\"FHIR Bundle generated.\")\\n",
    "\\n",
    "# Convert bundle to JSON dictionary\\n",
    "bundle_json = fhir_bundle.as_json()\\n",
    "\\n",
    "# Flatten Bundle to DataFrame\\n",
    "print(\"Flattening FHIR Bundle to DataFrame...\")\\n",
    "df = flatten_fhir_bundle(bundle_json)\\n",
    "print(\"DataFrame generated.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 4: Data Inspection (Raw DataFrame)\\n",
    "\\n",
    "print(\"--- Raw Flattened DataFrame Head (All Observations) ---\")\\n",
    "if df is not None and not df.empty:\\n",
    "    print(df.head().to_string())\\n",
    "else:\\n",
    "    print(\"DataFrame is empty or None.\\")\\n",
    "\\n",
    "# Optionally, inspect parts of the bundle_json\\n",
    "# print(\"\\\\n--- Sample of Bundle JSON ---\\")\\n",
    "# print(json.dumps({k: bundle_json[k] for k in list(bundle_json)[:3]}, indent=2)) # Print first 3 keys"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 5: Data Preparation for HDL Plotting\\n",
    "\\n",
    "hdl_df = pd.DataFrame() # Initialize empty DataFrame\\n",
    "\\n",
    "if df is not None and not df.empty:\\n",
    "    # Filter for HDL observations (LOINC code '2085-9')\\n",
    "    if 'Observation_code_coding_0_code' in df.columns:\\n",
    "        hdl_df = df[df['Observation_code_coding_0_code'] == '2085-9'].copy()\\n",
    "        print(f\"Filtered DataFrame `hdl_df` for HDL (code '2085-9') created. Shape: {hdl_df.shape}\")\\n",
    "    else:\\n",
    "        print(\"Warning: 'Observation_code_coding_0_code' column not found. Cannot filter for HDL.\")\\n",
    "\\n",
    "    if not hdl_df.empty:\\n",
    "        # Ensure 'Observation_effectiveDateTime' is in datetime format\\n",
    "        if 'Observation_effectiveDateTime' in hdl_df.columns:\\n",
    "            hdl_df['Observation_effectiveDateTime'] = pd.to_datetime(hdl_df['Observation_effectiveDateTime'])\\n",
    "            print(\"'Observation_effectiveDateTime' in hdl_df converted to datetime.\")\\n",
    "        else:\\n",
    "            print(\"Warning: 'Observation_effectiveDateTime' column not found in hdl_df.\")\\n",
    "\\n",
    "        # Ensure 'Observation_valueQuantity_value' is numeric\\n",
    "        if 'Observation_valueQuantity_value' in hdl_df.columns:\\n",
    "            hdl_df['Observation_valueQuantity_value'] = pd.to_numeric(hdl_df['Observation_valueQuantity_value'])\\n",
    "            print(\"'Observation_valueQuantity_value' in hdl_df ensured as numeric.\")\\n",
    "        else:\\n",
    "            print(\"Warning: 'Observation_valueQuantity_value' column not found in hdl_df.\")\\n",
    "    \\n",
    "        # Display prepared HDL data types (optional)\\n",
    "        print(\"\\\\nData types for hdl_df after preparation:\")\\n",
    "        if 'Observation_effectiveDateTime' in hdl_df.columns and 'Observation_valueQuantity_value' in hdl_df.columns:\\n",
    "            print(hdl_df[['Observation_effectiveDateTime', 'Observation_valueQuantity_value']].dtypes)\\n",
    "        elif 'Observation_effectiveDateTime' in hdl_df.columns:\\n",
    "            print(hdl_df[['Observation_effectiveDateTime']].dtypes)\\n",
    "        elif 'Observation_valueQuantity_value' in hdl_df.columns:\\n",
    "            print(hdl_df[['Observation_valueQuantity_value']].dtypes)\\n",
    "    else:\\n",
    "        print(\"hdl_df is empty after filtering. No HDL data to plot.\")\\n",
    "else:\\n",
    "    print(\"Initial DataFrame `df` is empty or None, skipping data preparation.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 6: Bokeh Plotting\\n",
    "\\n",
    "if hdl_df is not None and not hdl_df.empty and \\\\\\n",
    "   'Observation_effectiveDateTime' in hdl_df.columns and \\\\\\n",
    "   'Observation_valueQuantity_value' in hdl_df.columns and \\\\\\n",
    "   pd.api.types.is_datetime64_any_dtype(hdl_df['Observation_effectiveDateTime']) and \\\\\\n",
    "   pd.api.types.is_numeric_dtype(hdl_df['Observation_valueQuantity_value']):\\n",
    "\\n",
    "    p = figure(\\n",
    "        x_axis_type=\\"datetime\\",\\n",
    "        title=\\"HDL Cholesterol Over Time (Jupyter)\\",\\n",
    "        height=350,\\n",
    "        width=800,\\n",
    "        x_axis_label='Date',\\n",
    "        y_axis_label='HDL (mg/dL)'\\n",
    "    )\\n",
    "\\n",
    "    # Add a line and circle glyph\\n",
    "    p.line(\\n",
    "        x=hdl_df['Observation_effectiveDateTime'],\\n",
    "        y=hdl_df['Observation_valueQuantity_value'],\\n",
    "        legend_label=\\"HDL\\",\\n",
    "        line_width=2\\n",
    "    )\\n",
    "    p.circle(\\n",
    "        x=hdl_df['Observation_effectiveDateTime'],\\n",
    "        y=hdl_df['Observation_valueQuantity_value'],\\n",
    "        legend_label=\\"HDL\\", # Usually only need one legend entry for combined glyphs\\n",
    "        fill_color=\\"white\\",\\n",
    "        size=8\\n",
    "    )\\n",
    "\\n",
    "    # Format the x-axis to display dates nicely\\n",
    "    p.xaxis.formatter = DatetimeTickFormatter(\\n",
    "        days=\\"%Y-%m-%d\\",\\n",
    "        months=\\"%Y-%m\\",\\n",
    "        years=\\"%Y\\"\\n",
    "    )\\n",
    "    p.xaxis.axis_label_text_font_style = \\"normal\\"\\n",
    "    p.yaxis.axis_label_text_font_style = \\"normal\\"\\n",
    "\\n",
    "    # Customize plot appearance (optional)\\n",
    "    p.grid.grid_line_alpha = 0.3\\n",
    "    p.legend.location = \\"top_left\\"\\n",
    "\\n",
    "    # Show the plot\\n",
    "    show(p)\\n",
    "else:\\n",
    "    print(\"Could not generate plot. Please check if the DataFrame `hdl_df` is correctly populated and prepared, \\")\\n",
    "    print(\\"and if 'Observation_effectiveDateTime' and 'Observation_valueQuantity_value' columns are present \\")\\n",
    "    print(\\"with the correct data types.\\")"
   ]
  }
